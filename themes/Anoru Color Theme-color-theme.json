  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#b2b2b2"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "storage.type.class",
        "keyword.operator.expression.keyof.tsx",
        "keyword.operator.expression.typeof.tsx"
      ],
      "settings": {
        "foreground": "#B993B2"
      }
    },
    {
      "name": "strings",
      "scope": "string",
      "settings": {
        "foreground": "#A5C08F"
      }
    },
    {
      "name": "functions",
      "scope": "function",
      "settings": {
        "foreground": "#9BB0BE"
      }
    },
    {
      "name": "variables",
      "scope": ["variable", "meta.jsx.children"],
      "settings": {
        "foreground": "#DEDEDE"
      }
    },
    {
      "name": "types",
      "scope": [
        "type",
        "support.type",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "foreground": "#B0B5B9"
      }
    },
    {
      "name": "Name.Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#8be9ee" //"#18C9C9" // CHANGE
      }
    },
    {
      "name": "Generic: Heading (multi line)",
      "scope": "markup.heading.1 punctuation.definition.heading",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Name.Entity",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Exception",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.class.builtin",
        "keyword.other.name-of-parameter"
      ],
      "settings": {
        "foreground": "#9BB0BE"
      }
    },
    {
      "name": "Name.Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C4666F" // <div>
      }
    },
    {
      "name": "Support.Class",
      "scope": [
        "support.class",
        "support.class.component",
        //"meta.jsx.children",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "#C4666F" // <Field />
      }
    },
    {
      "name": "Support.Class",
      "scope": ["punctuation.terminator.statement"],
      "settings": {
        "foreground": "#DEDEDE" // <Field />
      }
    },
    {
      "name": "Component",
      "scope": ["entity.other.inherited-class", "entity.name.type"],
      "settings": {
        "foreground": "#B0B5B9"
      }
    },
    {
      "name": "Storage.Modifier", //EXTENDS
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#B993B2"
      }
    },
    {
      "name": "Storage.Type", // CONSTRUCTOR
      "scope": ["constructor"],
      "settings": {
        "foreground": "#9BB0BE"
      }
    },
    {
      "name": "Variable.Language",
      "scope": [
        "variable.language",
        "entity.other.attribute-name",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#B993B2",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant.Boolean",
      "scope": [
        "constant.language.boolean",
        "constant.numeric",
        "entity.other.attribute-name.tsx",
        "meta.tag.attributes.tsx"
      ],
      "settings": {
        "foreground": "#D58C75"
      }
    },
    {
      "name": "Variable.Parameter",
      "scope": [
        "variable.parameter",
        "variable.other.constant",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#DEDEDE"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        //"meta.tag",
        "meta.brace.round",
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#CDD3DE"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d7deea"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "punctuation.definition.comment",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b2b2b2"
      }
    }
  ]
}
